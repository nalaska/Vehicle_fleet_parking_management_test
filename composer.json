{
  "type": "project",
  "license": "proprietary",
  "require": {
    "behat/behat": "*",
    "symfony/console": "^7.2",
    "doctrine/orm": "^3.1",
    "symfony/cache": "^7.2",
    "ramsey/uuid": "^4.7",
    "ramsey/uuid-doctrine": "^2.1"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "allow-plugins": {
      "ocramius/package-versions": true,
      "symfony/flex": true
    }
  },
  "autoload": {
    "psr-4": {
      "Fulll\\": "src/",
      "Db\\": "bootstrap"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.73",
    "kubawerlos/php-cs-fixer-custom-fixers": "^3.24",
    "phpstan/phpstan": "^2.1"
  }
}
